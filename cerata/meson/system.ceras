prepare_system() {
  $RSYNC $SRCD/$nom/$nom-$ver/ $SSRC/$nom --delete
  cd $SSRC/$nom
}

configure_system() {
  :
}

build_system() {
  python3 setup.py \
    build
}

install_system() {
  python3 setup.py \
    install \
    --root=$SCER/$nom/sac
}
