prepare() {
  rsync -vah $CERD/$nom/$nom-$ver/ $SSRC/$nom --delete
  cd $SSRC/$nom
}

configure() {
  :
}

build() {
  make \
    PREFIX=/usr
}

install() {
  make \
    PREFIX=/usr \
    DESTDIR=$SCER/$nom/sac \
    install
}
