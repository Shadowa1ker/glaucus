# prepare
rsync -vah $CERD/$name/$name $TSRC --delete --exclude=.$version
cd $TSRC/$name

# configure
./autogen.sh
./configure \
  --prefix=$TOOL \
  --target=$TUPL \
  --disable-static \
  --with-sysroot=$TOOL/$TUPL \
  --with-gmp-prefix=$TOOL

# build
make

# install
make \
  install-strip
