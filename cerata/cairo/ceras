name=cairo
version=git
release=1
arch=x86_64
url=https://anon$git.freedesktop.org/$git/$name
cysts='musl zlib libpng pixman'
description=''
license=
build(){
        ./configure \
                --prefix=/usr \
                --build=$TUPL \
                --host=$TUPL \
                --target=$TUPL
        make
        make DESTDIR=$SCER/$name/sac \
                install-strip
}
