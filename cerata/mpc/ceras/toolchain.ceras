# prepare
rsync -vah $CERD/$name/$name $TSRC --delete
cd $TSRC/$name

# configure
autoreconf -vi
./configure \
  --prefix=$TOOL \
  --disable-static \
  --with-mpfr=$TOOL \
  --with-gmp=$TOOL

# build
make

# install
make \
  install-strip
