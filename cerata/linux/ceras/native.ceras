prepare_native() {
  install -dv $NBLD/$name

  cd $CERD/$name/$name

  make \
    mrproper

  make \
    O=$NBLD/$name \
    ARCH=$ARCH \
    headers_check
}

install_native() {
  make \
    O=$NBLD/$name \
    ARCH=$ARCH \
    INSTALL_HDR_PATH=$TOOL \
    headers_install

  find $TOOL/include \( -name ..install.cmd -o -name .install \) -delete
}
