nom=sway
ver=git
url=https://github.com/${nom}wm/$nom
lic=MIT
build(){
        $RSYNC $CERD/$nom/$nom $SSRC --delete
        cd $SSRC/$nom
        ./configure \
                --prefix=/usr \
                --build=$TUPL \
                --host=$TUPL \
                --target=$TUPL
        make
        make DESTDIR=$SCER/$nom/sac \
                install-strip
}
