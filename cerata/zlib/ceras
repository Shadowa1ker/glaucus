name=zlib
version=git
release=1
arch=x86_64
url=https://github.com/madler/$name
cyst=musl
description='A Massively Spiffy Yet Delicately Unobtrusive Compression Library'
license='BSLv1.0 ZLIB'
build(){
        mkdir -v $SBLD/$name
        cd $SBLD/$name
        $CERD/$name/$name/configure \
                --prefix= \
                --shared
        make
        make \
                DESTDIR=$SCER/$name/sac \
                install
}
