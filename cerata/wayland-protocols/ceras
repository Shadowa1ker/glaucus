nom=wayland-protocols
ver=git
url=https://gitlab.freedesktop.org/wayland/$nom
lic=MIT
build(){
        rsync -vah $CERD/$nom/$nom $SSRC --delete
        mkdir -v $SBLD/$nom
        cd $SBLD/$nom
        $SSRC/$nom/autogen.sh \
                --prefix=/usr
        make DESTDIR=$SCER/$nom/sac \
                install-strip
}
