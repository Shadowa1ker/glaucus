prepare_native() {
  $MKDIR $NBLD/$nom &&
  cd $NBLD/$nom
}

configure_native() {
  $SRCD/$nom/$nom-$ver/configure \
    --prefix=$TOOL \
    --zlib-compat
}

build_native() {
  make
}

install_native() {
  make \
    install
}
