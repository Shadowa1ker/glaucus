name=freetype
version=git
release=1
arch=x86_64
url=https://git.sv.nongnu.org/r/freetype/freetype2
cysts=
description=''
license=
build(){
        rsync -vah $CERD/$name/$name $SSRC --delete
        cd $SSRC/$name
        ./configure \
                --prefix=/usr \
                --build=$TUPL \
                --host=$TUPL \
                --target=$TUPL
        make
        make DESTDIR=$SCER/$name/sac \
                install-strip
}
