name=xcbproto
version=git
release=1
arch=x86_64
url=https://gitlab.freedesktop.org/xorg/proto/$name
cyst=musl
description='XML-XCB protocol descriptions used by libxcb for the X11 protocol & extensions'
license=MIT
build(){
        rsync -vah $CERD/$name/$name $SSRC --delete
        cd $SSRC/$name
        ./autogen.sh \
                --prefix=/usr \
                --build=$TUPL \
                --host=$TUPL \
                --target=$TUPL
        make
        make DESTDIR=$SCER/$name/sac \
                install-strip
}
