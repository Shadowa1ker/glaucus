verify() {
  for ceras in $@; do
    . $CERD/$ceras/ceras/ceras
}

verify m4
verify netbsd-curses
verify oksh
verify dash
verify bash
verify bison
verify lbzip2
verify toybox
verify rsync
verify diffutils
verify file
verify mawk
verify gettext-tiny
verify grep
verify zlib-ng
verify pigz
verify make
verify patch
verify perl
verify sed
verify xz
verify libarchive
verify texinfo
