build() {
  for ceras in $@; do
    . $CERD/$(echo $ceras | sed s/_.//)/ceras/ceras
    . $CERD/$(echo $ceras | sed s/_.//)/ceras/chroot.ceras
  done
}

build musl
build mawk_1 byacc mawk_2
build sbase lobase
build dash
build netbsd-curses loksh
build qm4 make
