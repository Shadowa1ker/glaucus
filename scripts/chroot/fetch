fetch() {
  for ceras in $@; do
    . $CERD/$ceras/ceras/ceras
}

fetch m4
fetch netbsd-curses
fetch oksh
fetch dash
fetch bash
fetch bison
fetch lbzip2
fetch toybox
fetch rsync
fetch diffutils
fetch file
fetch mawk
fetch gettext-tiny
fetch grep
fetch zlib-ng
fetch pigz
fetch make
fetch patch
fetch perl
fetch sed
fetch xz
fetch libarchive
fetch texinfo
