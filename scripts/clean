$UMOUNT $GLAD/dev ||
  true
$UMOUNT $GLAD/proc ||
  true
$UMOUNT $GLAD/sys ||
  true
$UMOUNT $GLAD/run ||
  true

$RM $GLAD/bin ||
  true
$RM $GLAD/boot ||
  true
$RM $GLAD/dev ||
  true
$RM $GLAD/etc ||
  true
$RM $GLAD/proc ||
  true
$RM $GLAD/root ||
  true
$RM $GLAD/run ||
  true
$RM $GLAD/sys ||
  true
$RM $GLAD/usr ||
  true
$RM $GLAD/var ||
  true
$RM $GLAD/tmp ||
  true
