construct() {
  . $CERD/$1/ceras &&
  . $CERD/$nom/cross.ceras &&
  
  prepare_cross || $SCRD/exit cross $nom prepare $? || exit &&
  configure_cross || $SCRD/exit cross $nom configure $? || exit &&
  build_cross || $SCRD/exit cross $nom build $? || exit &&
  install_cross || $SCRD/exit cross $nom install $? || exit
}

construct binutils &&
construct gcc
