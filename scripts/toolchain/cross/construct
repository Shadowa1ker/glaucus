construct() {
  . $CERD/$1/ceras &&
  . $CERD/$nom/cross.ceras &&
  
  prepare_cross || $SCRD/exit cross $nom prepare $? || return &&
  configure_cross || $SCRD/exit cross $nom configure $? || return &&
  build_cross || $SCRD/exit cross $nom build $? || return &&
  install_cross || $SCRD/exit cross $nom install $? || return
}

construct binutils &&
construct gcc
