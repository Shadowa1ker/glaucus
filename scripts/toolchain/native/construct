construct() {
  . $CERD/$1/ceras &&
  . $CERD/$nom/native.ceras &&
  
  prepare_native || $SCRD/exit native $nom prepare $? || return &&
  configure_native || $SCRD/exit native $nom configure $? || return &&
  build_native || $SCRD/exit native $nom build $? || return &&
  install_native || $SCRD/exit native $nom install $? || return
}

construct musl &&
construct libstdc++-v3 &&
construct binutils &&
construct gcc
