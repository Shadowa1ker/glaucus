construct() {
  . $CERD/$1/ceras &&
  . $CERD/$nom/native.ceras &&
  
  prepare_native || $SCRD/exit native $nom prepare $? || exit &&
  configure_native || $SCRD/exit native $nom configure $? || exit &&
  build_native || $SCRD/exit native $nom build $? || exit &&
  install_native || $SCRD/exit native $nom install $? || exit
}

construct linux-headers &&
construct musl &&
construct libstdc++-v3 &&
construct binutils &&
construct gcc
